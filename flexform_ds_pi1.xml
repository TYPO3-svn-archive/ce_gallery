          <thumbheightLarge>
            <TCEforms>
                <label>LLL:EXT:ce_gallery/locallang_db.xml:tx_ce_photogallery_thumbnails.thumbheightLarge</label>
                <config>
                    <type>input</type>
                    <size>10</size>
                    <default>600</default>
                </config>
            </TCEforms>
          </thumbheightLarge>
          <thumbqualityLarge>
              <TCEforms>
                  <label>LLL:EXT:ce_gallery/locallang_db.xml:tx_ce_photogallery_thumbnails.thumbqualityLarge</label>
                  <config>
                      <type>input</type>
                      <size>10</size>
                      <default>90</default>
                  </config>
              </TCEforms>

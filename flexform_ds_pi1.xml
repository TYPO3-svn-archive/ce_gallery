		      <orderby>
						<TCEforms>
							<label>LLL:EXT:ce_gallery/locallang_db.xml:tx_ce_photogallery_thumbnails.orderby</label>
							<config>
							  <type>select</type>
                   <items type="array">
                      <numIndex index="0" type="array">
                              <numIndex index="0">LLL:EXT:ce_gallery/locallang_db.xml:tx_ce_photogallery_thumbnails.orderby.uidasc</numIndex>
                              <numIndex index="1">tx_dam.uid ASC</numIndex>
                      </numIndex>
                      <numIndex index="1" type="array">
                              <numIndex index="0">LLL:EXT:ce_gallery/locallang_db.xml:tx_ce_photogallery_thumbnails.orderby.uiddesc</numIndex>

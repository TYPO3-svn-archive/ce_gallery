                      <numIndex index="4" type="array">
                              <numIndex index="0">LLL:EXT:ce_gallery/locallang_db.xml:tx_ce_photogallery_thumbnails.orderby.date_crasc</numIndex>
                              <numIndex index="1">tx_dam.date_cr ASC</numIndex>
                      </numIndex>                      
                      <numIndex index="5" type="array">
                              <numIndex index="0">LLL:EXT:ce_gallery/locallang_db.xml:tx_ce_photogallery_thumbnails.orderby.date_crdesc</numIndex>
                              <numIndex index="1">tx_dam.date_cr DESC</numIndex>
                      </numIndex>
                      <numIndex index="6" type="array">
                              <numIndex index="0">LLL:EXT:ce_gallery/locallang_db.xml:tx_ce_photogallery_thumbnails.orderby.date_modasc</numIndex>
                              <numIndex index="1">tx_dam.date_mod ASC</numIndex>
                      </numIndex>  
                      <numIndex index="7" type="array">
                              <numIndex index="0">LLL:EXT:ce_gallery/locallang_db.xml:tx_ce_photogallery_thumbnails.orderby.date_moddesc</numIndex>
                              <numIndex index="1">tx_dam.date_mod DESC</numIndex>
                      </numIndex>
                    </items>
                    <default>tx_dam.uid DESC</default>
							</config>
						</TCEforms>
		      </orderby>	
		      <randomAlbumImage>
						<TCEforms>
							<label>LLL:EXT:ce_gallery/locallang_db.xml:tx_ce_photogallery_thumbnails.randomAlbumImage</label>

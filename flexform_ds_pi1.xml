                      </numIndex>
                      <numIndex index="2" type="array">
                              <numIndex index="0">LLL:EXT:ce_gallery/locallang_db.xml:tx_ce_photogallery_thumbnails.orderby.titleasc</numIndex>
                              <numIndex index="1">tx_dam.title ASC</numIndex>
                      </numIndex>
                      <numIndex index="3" type="array">
